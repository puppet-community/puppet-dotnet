{
  "name":         "puppet-dotnet",
  "version":      "1.1.0-rc0",
  "author":       "Vox Pupuli",
  "license":      "MIT",
  "summary":      "Module to manage the Microsoft .NET framework",
  "source":       "https://github.com/voxpupuli/puppet-dotnet",
  "project_page": "https://github.com/voxpupuli/puppet-dotnet",
